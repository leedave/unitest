{
    "name": "leedch/unitest",
    "type": "library",
    "description": "Generates PHPUnit test classes from regular PHP classes",
    "keywords": [
        "leedch",
        "unit test",
        "phpunit",
        "codemonkey",
        "testing",
        "generate"
    ],
    "homepage": "https://github.com/leedave/unitest.git",
    "license": "MIT",
    "authors": [
        {
            "name": "David Lee",
            "email": "leed@leed.ch",
            "homepage": "https://www.leed.ch/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "leedch/html": "^1.0.0",
        "leedch/codemonkey": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "bin": [
        "unitest"
    ],
    "autoload": {
        "psr-4": {
            "Leedch\\Unitest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leedch\\Unitest\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}